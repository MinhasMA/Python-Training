  lst = []
  for i in range(1, n):
      if n % i == 0:
          lst.append(i)
  return lst
  print(lst)
  
def is_prime(n):

  ghaint = find_divisors(n)
  lengi = len(ghaint)
  if lengi>2:
    return False
  else:
    return True

def find_next_prime(n):
  ty = n+1
  while is_prime(ty) != True:
    ty +=1
  return ty

print(find_divisors(900))
print ("---------------------------------")
print(is_prime(3)) 
print(find_next_prime(11))
